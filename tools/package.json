        "eslint-plugin-vue": "^8.5.0",
        "husky": "^7.0.4",
        "jest": "^27.5.1",
        "jest-circus": "^27.5.1",
        "jest-cli": "^27.5.1",
        "lerna": "^4.0.0",
        "nativescript-vue": "~2.9.1",
        "ng-packagr": "~13.1.3",
        "ntl": "^5.1.0",
